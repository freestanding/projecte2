<main class="flex-shrink-0 mb-5 mt-4">
  <div class="container">
    <div class="p-5 rounded">
    </div>
  </div>
  <div class="row row-cols-1 row-cols-md-4 g-4">
    {{#if servicios_db}}
      {{#each servicios_db}}

        <div class="col">
          <div class="card">
            <img src="assets/img/{{foto}}" class="card-img-top" alt="..." />
            <div class="card-body">
              <h5 class="card-title">{{title}}</h5>
              <p class="card-text">
                {{desc}}
                <br /><br />
                <b>Precio:</b>
                {{precio}}€/año
              </p>
            </div>
          </div>
        </div>
      {{/each}}
    {{else}}
      <p>No Servicios Yet.</p>
    {{/if}}

  </div>
</main>